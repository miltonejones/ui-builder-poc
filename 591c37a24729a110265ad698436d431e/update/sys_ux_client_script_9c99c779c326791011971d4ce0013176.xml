        <script><![CDATA[function evaluateProperty({api, helpers}) {
  
  return !can('save');

  /**
   * Checks if a specific message can be triggered in the current machine state.
   * @param {string} message - The message to check.
   * @returns {boolean} - Whether the message can be triggered.
   */
  function can(message) {
    const { machineState, machine } = api.state;
    return Boolean(machine.states[machineState].on[message]);
  }
}]]></script>
